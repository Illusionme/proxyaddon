{
    "manifest_version":2,
    "name":"local proxy",
    "version":"1.0",
    
    "description":"Switches between local proxy and no proxy",
    "icons": {
        "48":"icons/beasts-48.png"
    },
    
    "permissions": [
        "browserSettings"
    ],
    
    "browser_action": {
        "default_icon": "icons/beasts-48.png",
        "default_title": "local proxy",
        "default_popup": "popup/switch.html"
    }
}
